<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookmarket_homework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookMarket_Homework">
        <activity
            android:theme="@style/Theme.BookMarket_Homework"
            android:label="도서상세정보"
            android:name=".BookInfoActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppBaseTheme"
            android:label=""
            android:exported="true"></activity>
        <activity
            android:theme="@style/Theme.BookMarket_Homework"
            android:label="도서 목록"
            android:name=".BooksActivity" />
        <activity
            android:name=".IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cart.CartActivity"/>
        <activity android:name=".order.OrderActivity"/>
        <activity android:name=".order.ShoppingActivity"/>
    </application>

</manifest>